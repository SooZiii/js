{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",

    "no-unused-vars": "off",

    "import/extensions": ["error", "ignorePackages", { "*.js": "never" }]
  },
  "ignorePatterns": ["**/*.html"]
}
